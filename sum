#include <stdio.h>
int add(int a,int b,int c,int d);
int main()
{
	int n1,n2,n3,n4,sum;
	printf("enter the four numbers of which the sum is to be found");
	scanf("%d%d%d%d",&n1,&n2,&n3,&n4);
	sum=add(n1,n2,n3,n4);
	printf("sum of digits=%d",sum);
	return 0;
}
int add(int a,int b,int c,int d)
{
	int result;
	result=a+b+c+d;
	return result;
}
